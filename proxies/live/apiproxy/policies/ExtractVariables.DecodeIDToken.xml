<ExtractVariables name="ExtractVariables.DecodeIDToken">
  <DisplayName>Decode ID Token</DisplayName>
  <JSONPayload>
    <Variable name="id_token">{request.content.id_token}</Variable>
  </JSONPayload>
  <VariablePrefix>decoded_</VariablePrefix>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <Source>request</Source>
</ExtractVariables>

<AssignMessage name="AssignMessage.GetIDToken">
  <DisplayName>Get ID Token</DisplayName>
  <AssignTo createNew="false">request</AssignTo>
  <Set>
    <Payload contentType="application/json">
      {
        "id_token": "{accesstoken.id_token}"
      }
    </Payload>
    <Var name="request.content">request.content</Var>
  </Set>
</AssignMessage>
